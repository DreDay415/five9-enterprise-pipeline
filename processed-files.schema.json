{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": ["processed"],
  "properties": {
    "processed": {
      "type": "array",
      "items": {
        "type": "object",
        "required": ["filename", "transcribed", "notion_uploaded"],
        "properties": {
          "filename": { "type": "string" },
          "transcribed": { "type": "string", "format": "date-time" },
          "notion_uploaded": { "type": "string", "format": "date-time" },
          "notion_page_id": { "type": "string" },
          "deleted": { "type": ["string", "null"], "format": "date-time" },
          "deletion_reason": { "type": ["string", "null"] },
          "deleted_from": { "type": ["string", "null"] }
        }
      }
    },
    "last_cleanup": { "type": ["string", "null"], "format": "date-time" },
    "disk_usage_before": { "type": ["string", "null"] },
    "disk_usage_after": { "type": ["string", "null"] }
  }
}
